{
	"name": "valetudo-re",
	"version": "0.10.0",
	"versionSuffix": "",
	"description": "Self-contained control webinterface for xiaomi vacuum robots",
	"main": "index.js",
	"bin": "index.js",
	"license": "Apache-2.0",
	"pkg": {
		"assets": [
			"client/**/*"
		]
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"prebuild": "cp -a client client.b && find client/ -type f \\( -iname \\*.json -o -iname \\*.js -o -iname \\*.html -o -iname \\*.css \\) -exec gzip -9 {} +",
		"build": "pkg --targets node14-linux-armv7 --no-bytecode --options expose-gc,max-heap-size=30,optimize-for-size,always-compact,lite-mode --public-packages=prettycron . --output valetudo",
		"postbuild": "if [ -d client.b ]; then rm -fr client && mv client.b client; fi"
	},
	"author": "",
	"dependencies": {
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"express": "^4.17.1",
		"express-basic-auth": "^1.2.0",
		"express-force-ssl": "^0.3.2",
		"express-dynamic-middleware": "^1.0.0",
		"mqtt": "^4.2.6",
		"multer": "^1.4.2",
		"pngjs": "^5.0.0",
		"prettycron": "^0.10.0",
		"slimbot": "^4.4.0",
		"socks5-client": "^1.2.8",
		"ws": "^7.4.1"
	},
	"devDependencies": {
		"pkg": "4.4.9",
		"cross-env": "7.0.2"
	}
}
